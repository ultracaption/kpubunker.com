<%= render partial: 'shared/menu' %>
<div class="container">
  <div id="content_left_wrapper">
    <% if @result.empty? %>
      검색 결과가 없습니다.
    <% else %>
      <% @result.each do |lecture| %>
        <%= render partial: 'shared/lecture', locals: { lecture: lecture } %>
      <% end %>
    <% end %>

    <%= will_paginate @result, :params => { :query => @query } %>
  </div>

</div>
